<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectofinal">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFinal">
        <activity
            android:name=".InicioUsuario"
            android:exported="false" />
        <activity
            android:name=".pruebas"
            android:exported="false" />
        <activity
            android:name=".RecuperarPassword"
            android:exported="false" />
        <activity
            android:name=".UsuarioActivity"
            android:exported="false"
            android:label="@string/title_activity_usuario"
            android:theme="@style/Theme.ProyectoFinal.NoActionBar" />
        <activity
            android:name=".ActivityAdministrador"
            android:exported="false"
            android:label="@string/title_activity_administrador"
            android:theme="@style/Theme.ProyectoFinal.NoActionBar" />
        <activity
            android:name=".ActivityRegistrar"
            android:exported="false" />
        <activity
            android:name=".ActivityIngresar"
            android:exported="false" />
        <activity
            android:name=".ActivityAdmin"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>